{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DetailedErrors": false,
  "ConnectionStrings": {
    "DefaultConnection": ""
  },

  "Seed": {
    "Enabled": false
  },

  "CorsSettings": {
    "AllowedOrigins": [],
    "AllowedMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
    "AllowedHeaders": ["Content-Type", "Authorization"],
    "ExposedHeaders": ["X-Custom-Header"],
    "AllowCredentials": true,
    "PreflightMaxAge": 600
  },
  "JwtSettings": {
    "SecretKey": "",
    "Issuer": "",
    "Audience": "",
    "ExpireMinutes": 0
  },
  "AllowedHosts": "*"
}
