{
    "version": 2,
    "name": "service-ai-analyzer",
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "regions": ["cle1"],
    "github": {
        "silent": true
    }
}
